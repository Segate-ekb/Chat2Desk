;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	C2D_Клиенты.Клиент КАК Клиент,
	C2D_Клиенты.Представление КАК Представление,
	C2D_Клиенты.name КАК ИмяКлиента,
	C2D_Клиенты.email КАК emailКлиента,
	C2D_Клиенты.phone КАК ИдАккаунтаКлиента,
	C2D_Клиенты.avatar КАК СсылкаНаАватарКлиента,
	C2D_Клиенты.client_phone КАК ТелефонКлиента,
	C2D_Клиенты.assigned_name КАК НазначеноеИмяКлиента,
	C2D_Операторы.Пользователь КАК Оператор,
	C2D_Операторы.first_name + &quot; &quot; + C2D_Операторы.last_name КАК ФиоОператора,
	C2D_Сообщения.Текст КАК Текст,
	ВЫБОР
		КОГДА C2D_Диалоги.transport = &quot;whatsapp&quot;
			ТОГДА 0
		КОГДА C2D_Диалоги.transport = &quot;telegram&quot;
			ТОГДА 1
		КОГДА C2D_Диалоги.transport = &quot;facebook&quot;
			ТОГДА 2
		КОГДА C2D_Диалоги.transport = &quot;vk&quot;
			ТОГДА 3
		КОГДА C2D_Диалоги.transport ПОДОБНО &quot;%insta%&quot;
			ТОГДА 4
		КОГДА C2D_Диалоги.transport = &quot;wechat&quot;
			ТОГДА 5
		КОГДА C2D_Диалоги.transport ПОДОБНО &quot;%viber%&quot;
			ТОГДА 7
		КОГДА C2D_Диалоги.transport = &quot;skype&quot;
			ТОГДА 8
		КОГДА C2D_Диалоги.transport = &quot;ok&quot;
			ТОГДА 9
		КОГДА C2D_Диалоги.transport = &quot;email&quot;
			ТОГДА 12
		КОГДА C2D_Диалоги.transport = &quot;widget&quot;
			ТОГДА 14
		иначе 14	
	КОНЕЦ КАК Транспорт,
	C2D_Диалоги.state КАК Состояние,
	C2D_Диалоги.id КАК dialog_id,
	C2D_Клиенты.ID КАК client_id,
	C2D_Сообщения.Id КАК message_Id,
	C2D_Операторы.ID КАК operator_id,
	C2D_Сообщения.ДатаСообщения КАК ДатаСообщения
ПОМЕСТИТЬ ПрИтог
ИЗ
	РегистрСведений.C2D_Операторы КАК C2D_Операторы
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.C2D_Диалоги КАК C2D_Диалоги
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.C2D_Клиенты КАК C2D_Клиенты
			ПО C2D_Диалоги.client_id = C2D_Клиенты.ID
			ЛЕВОЕ СОЕДИНЕНИЕ ВТСообщенийПоДиалогам КАК ВТСообщенийПоДиалогам
				ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.C2D_Сообщения КАК C2D_Сообщения
				ПО ВТСообщенийПоДиалогам.Message_id = C2D_Сообщения.Id
			ПО C2D_Диалоги.id = ВТСообщенийПоДиалогам.dialog_id
		ПО C2D_Операторы.ID = C2D_Диалоги.operator_id
ГДЕ
	C2D_Операторы.Пользователь В(&amp;Пользователь)
;

